{
  "parserOptions": {
    "ecmaVersion": "2016"
  },
  "env": {
    "node": true,
    "es6": true
  },
  "rules": {
    "comma-spacing": 2,
    "complexity": [2, 10],
    "dot-location": [2, "property"],
    "eqeqeq": [2, "smart"],
    "max-depth": [2, 3],
    "new-cap": [2, { "capIsNewExceptions": ["Q", "Router", "STRING", "Q.Promise"] }],
    "no-alert": 2,
    "no-caller": 2,
    "no-cond-assign": [2, "except-parens"],
    "no-constant-condition": 2,
    "no-delete-var": 2,
    "no-duplicate-case": 2,
    "no-empty-character-class": 2,
    "no-eval": 2,
    "no-ex-assign": 2,
    "no-extra-bind": 2,
    "no-extra-semi": 2,
    "no-fallthrough": 2,
    "no-floating-decimal": 2,
    "no-func-assign": 2,
    "no-implied-eval": 2,
    "no-inner-declarations": 2,
    "no-labels": 0,
    "no-lone-blocks": 2,
    "no-lonely-if": 2,
    "no-loop-func": 2,
    "no-negated-in-lhs": 2,
    "no-new": 2,
    "no-obj-calls": 2,
    "no-return-assign": 2,
    "no-self-compare": 2,
    "no-sparse-arrays": 2,
    "no-throw-literal": 2,
    "no-undef": 2,
    "no-unneeded-ternary": 2,
    "no-unreachable": 2,
    "no-unused-expressions": 2,
    "no-unused-vars": 2,
    "no-use-before-define": [2, "nofunc"],
    "no-useless-call": 2,
    "radix": 2,
    "semi": [2, "always"],
    "semi-spacing": 2,
    "strict": [2, "global"],
    "use-isnan": 2,
    "valid-typeof": 2,
    "wrap-iife": [2, "any"]
  }
}
